<%-include('../partials/header.html')%>
    <link rel="stylesheet" href="/../../css/admin.css">
    <title>Admin Profile</title>
    </head>

    <body>

        <%-include('partials/navbar.html')%>
            <div class="container-fluid">
                <div class="row">
                    <%-include('partials/sidebar.html')%>
                        <div class="col-md-10 content">
                            <div class="row">
                                <div class="col-md-6 profile-content">
                                    <p class="display-4">Your Profile</p>
                                    <div class="form-group row">
                                        <label class="col-sm-2 col-form-label">First Name</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" readonly value="<%=res.firstName%>">
                                        </div>
                                    </div>
                                    <hr>
                                    <label class="col-sm-2 col-form-label">Last Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" readonly value="<%=res.lastName%>">
                                    </div>
                                    </div>
                                    <hr>
                                    <div class="form-group row">
                                        <label class="col-sm-2 col-form-label">Email</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" readonly value="<%=res.email%>">
                                        </div>
                                    </div>
                           
                                    <hr>
                                 

                                    <a href="/admin/profile/edit">Edit Profile</a> |
                                    <a href="/admin/changepass">Change Password</a>

                            
                            </div>
                        </div>
                </div>
            </div>

            <%-include('../partials/footer.html')%>
